    "webpack-manifest-plugin": "^1.1.1"
  },
  "engines": {
    "node": ">=6.9.1",
    "npm": "^3.10.8"
  },
  "jest": {
    "setupFiles": [
      "./test.setup.js"
    ],
    "coverageReporters": [
      "json-summary",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!src/theme/**"
    ]
  }
}
