{
  "name": "Mobile",
  "version": "0.0.1",
  "description": "Mobile",
  "scripts": {
    "start": "node ./server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.production.config.js",
    "build:deploy": "npm run build && npm run deploy",
    "build:deployProduction": "npm run build && npm run deployProduction",
    "build:e2e": "node msbuild.js ../../Test/Mobile.sln",
    "deployLogin": "node deployLogin.js",
    "deploy": "cross-env copyfiles -f {./distNoRx/*.*,./src/config.js} /inetpub/wwwroot/SRSUI/Mobile/static && cross-env copyfiles -f ./distNoRx/index.html /inetpub/wwwroot/SRSUI/Mobile && cross-env copyfiles -f ./distNoRx/silent_renew.html /inetpub/wwwroot/SRSUI/Mobile",
    "deployProduction": "cross-env copyfiles -f ./distNoRx/*.* //devanywhere.srs.com/C$/SRSServer/WWW/SRSUI/Mobile/static && cross-env copyfiles -f ./distNoRx/index.html //devanywhere.srs.com/C$/SRSServer/WWW/SRSUI/Mobile",
    "test": "jest --forceExit",
    "test:watch": "npm run test -- --watch",
    "e2e": "cross-env BASE_URI=http://localhost:3000/ nunit3-console ../../Test/MobileAutoTest/bin/Debug/MobileAutoTest.dll",
    "cover": "jest --coverage --forceExit --runInBand",
    "cover:checkPrev": "node ./compareCoverageToPrev.js",
    "cover:reportToSlack": "node ./reportCoverageToSlack.js",
    "cover:deploy": "npm run build && npm run cover && npm run coverDeploy",
    "coverDeploy": "cross-env copyfiles {./distNoRx/stats.html,./coverage/**/*.*} //srsdevwiki.srs.com/C$/inetpub/wwwroot/Mobile/docsBuild",
    "lint": "cross-env NODE_ENV=development eslint --ext .js,.jsx src/**",
    "lint:fix": "npm run lint -- --fix",
    "buildDocs": "rmdir docsBuild /s /q && npm run buildCredits && npm run buildLicenses && cross-env NODE_ENV=production marked-directory {./docs/**/*.md,./*.md} ./docsBuild && cross-env copyfiles {./docs/**/*.png,./docs/**/*.gif} ./docsBuild && npm run setIssueCount",
    "buildDocs:deploy": "npm run buildDocs && cross-env copyfiles ./docsBuild/**/*.* //srsdevwiki.srs.com/C$/inetpub/wwwroot/Mobile",
    "buildPresentation:security": "cd docs/presentations/security && npm install && npm run build",
    "buildPresentation:security:deploy": "cross-env copyfiles -u 4 ./docs/presentations/security/dist/*.* //srsdevwiki.srs.com/C$/inetpub/wwwroot/Mobile/docsBuild/presentations/security/dist && cross-env copyfiles -u 3 ./docs/presentations/security/index.html //srsdevwiki.srs.com/C$/inetpub/wwwroot/Mobile/docsBuild/presentations/security/",
    "buildLicenses": "node ./buildLicenses.js > ./docs/ModuleLicenses.md",
    "buildCredits": "cross-env NODE_ENV=production credits . --reporter markdown > ./docs/THANKS.md",
    "setIssueCount": "node ./bugCount.js"
  },
  "private": true,
  "license": "UNLICENSED",
  "dependencies": {
    "@srssoft/react-pdfjs": "^1.0.8",
